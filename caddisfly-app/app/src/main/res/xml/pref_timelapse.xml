<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <org.akvo.caddisfly.preference.PreferenceCategoryCustom android:title="@string/coliforms">
        <Preference
            android:key="startTurbidityTest"
            android:title="Coliform Test"
            android:icon="@drawable/ic_bug_report_black_24dp">
            <intent
                android:data="df3d1009-2112-4d95-a6f9-fdc4b5633ec9"
                android:targetClass="org.akvo.caddisfly.sensor.turbidity.TimeLapseActivity"
                android:targetPackage="org.akvo.caddisfly.experiment" />
        </Preference>
        <Preference
            android:key="analyzeColiform"
            android:title="View Coliform Report"
            android:icon="@drawable/ic_insert_chart_black_24dp">
            <intent
                android:targetClass="org.akvo.caddisfly.sensor.turbidity.AnalyseActivity"
                android:targetPackage="org.akvo.caddisfly.experiment" />
        </Preference>

    </org.akvo.caddisfly.preference.PreferenceCategoryCustom>
</PreferenceScreen>
